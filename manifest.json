{
  "browser_specific_settings": {
    "gecko": {
      "id": "terra-zen@aednichols.com"
    }
  },
  "content_scripts": [
    {
      "js": [
        "zen.js"
      ],
      "matches": [
        "*://app.terra.bio/*",
        "*://bvdp-saturn-dev.appspot.com/*",
        "*://bvdp-saturn-alpha.appspot.com/*",
        "*://bvdp-saturn-perf.appspot.com/*",
        "*://bvdp-saturn-staging.appspot.com/*",
        "*://localhost/*"
      ]
    }
  ],
  "description": "Hides Terra notifications",
  "manifest_version": 2,
  "name": "Terra Zen",
  "version": "1.4",
  "icons": {
    "16": "turtle_16.png",
    "48": "turtle_48.png",
    "96": "turtle.svg",
    "128": "turtle_128.png"
  }
}